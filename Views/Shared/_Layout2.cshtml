<!doctype html>
<html lang="en" dir="ltr">

<partial name="~/Views/Shared/Partials/_Head.cshtml" />


<body class="geex-dashboard">

	<partial name="~/Views/Shared/Partials/_Header.cshtml" />



	<main class="geex-main-content">

		<partial name="~/Views/Shared/Partials/_Sidebar.cshtml" />

		<partial name="~/Views/Shared/Partials/_Customizer.cshtml" />

		<div class="geex-content">
			@RenderBody()
		</div>
	</main>

	<!-- JAVASCRIPTS START -->
	<script src="~/vendor/js/jquery/jquery-3.5.1.min.js"></script>
	<script src="~/vendor/js/jquery/jquery-ui.js"></script>
	<script src="~/vendor/js/bootstrap/bootstrap.min.js"></script>
	<script src="~/js/main.js"></script>
	<!-- Page-specific scripts -->
	@await RenderSectionAsync("Scripts", required: false)
	<!-- JAVASCRIPTS END -->
</body>

</html>